<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length>0){ %>
            <ul>
           <% for(let order of orders){ %>
              <li>
                <div><%= order._id %></div>
                 <ul >
                  <%for(let product of order.items){%>
                    <div><%= product.product.title %> (<%=  product.quantity %>)</div>
                  <%}%>
                </ul>
              </li>
           <% } %>
            </ul>
           <% }else{ %>
                <h1>Nothing there!</h1>
            <% } %>
            
        </main>
        <%- include('../includes/end.ejs') %>